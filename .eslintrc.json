{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es2021": true,
    "mocha": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-param-reassign": "off",
    "no-use-before-define": "off"
  }
}
